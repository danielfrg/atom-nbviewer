{
  "name": "nbviewer",
  "main": "./lib/nbviewer",
  "version": "1.2.0",
  "description": "Jupyter Notebook Viewer",
  "keywords": [
    "Jupyter",
    "Notebook"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danielfrg/atom-nbviewer"
  },
  "license": "Apache 2.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
